# .env file content (REPLACE WITH YOUR VALUES!)

# --- Database (From Coolify) ---
POSTGRES_SERVER=YOUR_COOLIFY_DB_HOST_OR_SERVICE_NAME
POSTGRES_USER=YOUR_COOLIFY_DB_USER
POSTGRES_PASSWORD=YOUR_COOLIFY_DB_PASSWORD
POSTGRES_DB=YOUR_COOLIFY_DB_NAME
POSTGRES_PORT=5432 # Or Coolify's port if different

# --- Proxies (From Smartproxy) ---
PROXY_ENABLED=True
PROXY_HOST=gate.smartproxy.com # Or your specific endpoint
PROXY_PORT=10000 # Or your specific port
PROXY_USER=YOUR_SMARTPROXY_USER
PROXY_PASSWORD=YOUR_SMARTPROXY_PASSWORD

# --- LLM Provider (e.g., OpenAI) ---
LLM_INFERENCE_ENDPOINT=https://api.openai.com/v1/chat/completions # Example
LLM_API_KEY=YOUR_OPENAI_API_KEY # Or other provider's key

# --- Security ---
ENCRYPTION_KEY=YOUR_GENERATED_32_BYTE_HEX_KEY_HERE # Paste generated key

# --- Payment (From Lemon Squeezy) ---
LEMONSQUEEZY_API_KEY=YOUR_LEMONSQUEEZY_API_KEY
LEMONSQUEEZY_STORE_ID=YOUR_LEMONSQUEEZY_STORE_ID

# --- Signal Sources (Optional) ---
NEWS_API_KEY=YOUR_NEWSAPI_ORG_KEY # Optional

# --- MCOL Configuration ---
MCOL_IMPLEMENTATION_MODE=SUGGEST # Start with SUGGEST for safety, change to ATTEMPT_EXECUTE later if desired

# --- Other Settings (Defaults are usually fine) ---
# REPORT_GENERATOR_INTERVAL_SECONDS=15
# PROSPECT_RESEARCHER_INTERVAL_SECONDS=3600
# EMAIL_MARKETER_INTERVAL_SECONDS=30
# EMAIL_BATCH_SIZE=5
# MCOL_ANALYSIS_INTERVAL_SECONDS=300
# OPEN_DEEP_RESEARCH_REPO_PATH=/app/open-deep-research
# OPEN_DEEP_RESEARCH_ENTRY_POINT=main.js # Verify this in the repo
# NODE_EXECUTABLE_PATH=node